@online{goldsteinUnderstandingEffectivenessContent2023,
  title = {Understanding the ({{In}}){{Effectiveness}} of {{Content Moderation}}: {{A Case Study}} of {{Facebook}} in the {{Context}} of the {{U}}.{{S}}. {{Capitol Riot}}},
  shorttitle = {Understanding the ({{In}}){{Effectiveness}} of {{Content Moderation}}},
  author = {Goldstein, Ian and Edelson, Laura and Nguyen, Minh-Kha and Goga, Oana and McCoy, Damon and Lauinger, Tobias},
  date = {2023-02-21},
