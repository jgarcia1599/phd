@online{chenUnderstandingHowPeople2022,
  title = {Understanding How People Consume Low Quality and Extreme News Using Web Traffic Data},
  author = {Chen, Zhouhan and Chen, Haohan and Freire, Juliana and Nagler, Jonathan and Tucker, Joshua A.},
