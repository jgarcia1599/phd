  keywords = {Computer Science - Artificial Intelligence,Computer Science - Human-Computer Interaction,misinformation,safety,values},
  file = {/Users/juniorgarcia/Zotero/storage/7F2JJFA2/Jia et al. - 2023 - Embedding Democratic Values into Social Media AIs .pdf;/Users/juniorgarcia/Zotero/storage/3TWGALUA/2307.html}
